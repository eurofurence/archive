{
  "name": "ef-archive",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.2.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.71.5"
  },
  "devDependencies": {
    "purify-css": "^1.2.5",
